{
  "name": "vue-app",
  "version": "0.0.0",
  "description": "Bulletproof Vue 3 SPA Template",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "packageManager": "pnpm@8.10.2",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .vue,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "typesafe-i18n": "typesafe-i18n",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "dev": "vite",
    "start": "npm-run-all --parallel dev typesafe-i18n",
    "build-only": "pnpm msw:delete && vite build",
    "build-only:staging": "pnpm msw:delete && vite build --mode staging",
    "build": "npm-run-all typecheck build-only",
    "build:staging": "npm-run-all typecheck build-only:staging",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage"
  },
  "devDependencies": {
    "@commitlint/cli": " ^18.4.4",
    "@commitlint/config-conventional": " ^18.4.4",
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": " ^1.6.1",
    "@tailwindcss/forms": " ^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/eslint-plugin-query": " ^5.17.7",
    "@testing-library/jest-dom": " ^6.2.0",
    "@testing-library/user-event": " ^14.5.2",
    "@testing-library/vue": " ^8.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": " ^21.1.6",
    "@types/node": "^18.18.9",
    "@vitejs/plugin-vue": " ^5.0.2",
    "@vitest/coverage-istanbul": " ^1.1.3",
    "@vitest/ui": " ^1.1.3",
    "@vue/eslint-config-prettier": " ^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": " ^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": " ^8.56.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-tailwindcss": " ^3.13.1",
    "eslint-plugin-testing-library": " ^6.2.0",
    "eslint-plugin-vue": " ^9.19.2",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "husky": "^8.0.3",
    "jsdom": " ^23.1.0",
    "msw": " ^2.0.12",
    "npm-run-all": "^4.1.5",
    "postcss": " ^8.4.33",
    "prettier": " ^3.1.1",
    "tailwindcss": " ^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": " ^10.9.2",
    "typescript": " ~5.3.3",
    "vite": " ^5.0.11",
    "vite-tsconfig-paths": " ^4.2.3",
    "vitest": " ^1.1.3",
    "vue-tsc": " ^1.8.27"
  },
  "dependencies": {
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tanstack/vue-query": " ^5.17.7",
    "@tanstack/vue-query-devtools": " ^5.17.7",
    "@vee-validate/zod": " ^4.12.4",
    "@vueuse/core": "^10.7.1",
    "axios": "^1.6.5",
    "pinia": "^2.1.7",
    "primevue": " ^3.45.0",
    "tailwind-merge": " ^2.2.0",
    "type-fest": " ^4.9.0",
    "typesafe-i18n": "^5.26.2",
    "vee-validate": " ^4.12.4",
    "vue": " ^3.4.5",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  }
}
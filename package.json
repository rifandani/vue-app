{
  "name": "vue-app",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.14.1",
  "description": "Bulletproof Vue 3 SPA Template",
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "typesafe-i18n": "typesafe-i18n",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "dev-only": "vite",
    "dev": "run-p dev-only typesafe-i18n",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "build": "run-p typecheck build-only",
    "build:staging": "run-p typecheck build-only:staging",
    "preview": "vite preview",
    "release": "bumpp"
  },
  "dependencies": {
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tanstack/vue-query": "^5.17.11",
    "@tanstack/vue-query-devtools": "^5.17.11",
    "@vee-validate/zod": " ^4.12.4",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.5",
    "npm-run-all2": "^6.1.1",
    "pinia": "^2.1.7",
    "primevue": "^3.46.0",
    "tailwind-merge": " ^2.2.0",
    "type-fest": " ^4.9.0",
    "typesafe-i18n": "^5.26.2",
    "vee-validate": " ^4.12.4",
    "vue": "^3.4.13",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@commitlint/cli": " ^18.4.4",
    "@commitlint/config-conventional": " ^18.4.4",
    "@iconify/vue": "^4.1.1",
    "@tailwindcss/forms": " ^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": " ^6.2.0",
    "@testing-library/user-event": " ^14.5.2",
    "@testing-library/vue": " ^8.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": " ^21.1.6",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-istanbul": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "@vue/tsconfig": " ^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": " ^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "msw": "^2.0.14",
    "pnpm": "^8.14.1",
    "postcss": " ^8.4.33",
    "prettier": "^3.2.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": " ^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": " ^10.9.2",
    "typescript": " ~5.3.3",
    "vite": " ^5.0.11",
    "vite-tsconfig-paths": " ^4.2.3",
    "vitest": "^1.2.0",
    "vue-tsc": " ^1.8.27"
  }
}
